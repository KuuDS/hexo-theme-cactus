<footer id="footer">
  <div class="footer-up">
    <div class="footer-center">
      <nav>
        <ul>
          <% for (var i in theme.nav) { %>
            <li><a href="<%- url_for(theme.nav[i]) %>">
                <%= __('nav.'+i).replace("nav.", "") %>
              </a></li>
          <% } %>
        </ul>
      </nav>
    </div>
  </div>
  <div class="footer-down">
    <div class="footer-left">
      <%= __('footer.copyright') %> &copy;
      <% var endYear = (theme.copyright && theme.copyright.end_year) ? theme.copyright.end_year : new Date().getFullYear() %>
      <% var startYear = (theme.copyright && theme.copyright.start_year) ? theme.copyright.start_year : new Date().getFullYear() %>
      <%= startYear >= endYear ? endYear : startYear + "-" + endYear %>
      <%= config.author || config.title %>
    </div>
    <div class="footer-right">
      <span>
        <a target="_blank" href="http://beian.miit.gov.cn/"><%= theme.beian.icp_no %></a>
      </span>
      <span>
        <img src="<%- theme.root %>/images/police.png" style="height: 1em; vertical-align: middle; margin-right: 5px; background: transparent;">
        <a href="https://beian.mps.gov.cn/#/query/webSearch?code=<%- theme.beian.public_no.replace(/\D/g, '') %>" rel="noreferrer" target="_blank">
          <%= theme.beian.public_no %>
        </a>
      </span>
    </div>
  </div>
</footer>